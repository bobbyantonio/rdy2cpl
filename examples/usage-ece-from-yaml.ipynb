   "source": [
    "import yaml\n",
    "from pprint import pprint\n",
    "\n",
    "from namcouple import from_dict, reduce"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "078344b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('ece-namcouple.yaml') as f:\n",
    "    nc = yaml.load(f, Loader=yaml.FullLoader)\n",
    "# pprint(nc)\n",
    "nc = from_dict(nc)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "e17bc740",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "# The EC-Earth4 namcouple as an example\n",
      "\n",
      "$NFIELDS\n",
      "    8\n",
      "\n",
      "$RUNTIME\n",
      "    31536000\n",
      "\n",
      "$NLOGPRT\n",
      "    1 0\n",
      "\n",
      "$NNOREST\n",
      "    True\n",
      "\n",
      "$STRINGS\n",
      "\n",
      "# Momentum fluxes for oce and ice on U grid\n",
      " A_TauX_oce:A_TauY_oce:A_TauX_ice:A_TauY_ice O_OTaux1:O_OTauy1:O_ITaux1:O_ITauy1 1 2700 2 rstat.nc EXPORTED\n",
      " IOCL NOUM LAG=2700\n",
      " P 0 P 2\n",
      " LOCTRANS SCRIPR\n",
      "  AVERAGE\n",
      "  GAUSWGT D SCALAR LATITUDE 1 9 2.0\n",
      "\n",
      "# Momentum fluxes for oce and ice on V grid\n",
      " A_TauX_oce:A_TauY_oce:A_TauX_ice:A_TauY_ice O_OTaux2:O_OTauy2:O_ITaux2:O_ITauy2 1 2700 2 rstat.nc EXPORTED\n",
      " IOCL NOVM LAG=2700\n",
      " P 0 P 2\n",
      " LOCTRANS SCRIPR\n",
      "  AVERAGE\n",
      "  GAUSWGT D SCALAR LATITUDE 1 9 2.0\n",
      "\n",
      "# Non-solar and solar radiation over ocean+ice, total evaporation, precipitation (conserved, preserved sign)\n",
      " A_Qns_mix:A_Qs_mix:A_Evap_total:A_Precip_liquid:A_Precip_solid O_QnsMix:O_QsrMix:OTotEvap:OTotRain:OTotSnow 1 2700 3 rstat.nc EXPORTED\n",
      " IOCL NOTM LAG=2700\n",
      " P 0 P 2\n",
      " LOCTRANS SCRIPR CONSERV\n",
      "  AVERAGE\n",
      "  GAUSWGT D SCALAR LATITUDE 1 9 2.0\n",
      "  GLBPOS opt\n",
      "\n",
      "# Solar/non-solar radiation over ice, dQns/dT, evaporation over ice (not conserved)\n",
      " A_Qs_ice:A_Qns_ice:A_dQns_dT:A_Evap_ice O_QsrIce:O_QnsIce:O_dQnsdT:OIceEvap 1 2700 2 rstat.nc EXPORTED\n",
      " IOCL NOTM LAG=2700\n",
      " P 0 P 2\n",
      " LOCTRANS SCRIPR\n",
      "  AVERAGE\n",
      "  GAUSWGT D SCALAR LATITUDE 1 9 2.0\n",
      "\n",
      "# Runoff and Calving\n",
      " A_Runoff R_Runoff_atm 1 2700 3 rstat.nc EXPORTED\n",
      " ILCL RNFA LAG=2700\n",
      " P 0 P 0\n",
      " LOCTRANS SCRIPR CONSERV\n",
      "  AVERAGE\n",
      "  GAUSWGT D SCALAR LATITUDE 1 9 2.0\n",
      "  GLBPOS opt\n",
      "\n",
      "# Runoff\n",
      " R_Runoff_oce O_Runoff 1 2700 3 none EXPORTED\n",
      " RNFA NOTM\n",
      " P 0 P 2\n",
      " SCRIPR CONSERV BLASNEW\n",
      "  GAUSWGT LR SCALAR LATITUDE 1 9 2.0\n",
      "  GLBPOS opt\n",
      "  1.07945 0\n",
      "\n",
      "# Calving\n",
      " R_Calving_oce OCalving 1 2700 2 none EXPORTED\n",
      " RNFA NOTM\n",
      " P 0 P 2\n",
      " SCRIPR CONSERV\n",
      "  GAUSWGT LR SCALAR LATITUDE 1 9 2.0\n",
      "  GLBPOS opt\n",
      "\n",
      "# SST, ice temperature, albedo, fraction, thickness; snow thickness over ice\n",
      " O_SSTSST:O_TepIce:O_AlbIce:OIceFrc:OIceTck:OSnwTck A_SST:A_Ice_temp:A_Ice_albedo:A_Ice_frac:A_Ice_thickness:A_Snow_thickness 1 2700 2 rstos.nc EXPORTED\n",
      " NOTM IOCL LAG=2700\n",
      " P 2 P 0\n",
      " LOCTRANS SCRIPR\n",
      "  AVERAGE\n",
      "  GAUSWGT LR SCALAR LATITUDE 1 9 2.0\n",
      "\n",
      "$END\n",
      "\n"
     ]
    }
   ],
   "source": [
    "print(nc.out)"
   ]
  },
